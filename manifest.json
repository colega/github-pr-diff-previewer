{
  "manifest_version": 3,
  "name": "Github PR Diff Previewer",
  "version": "1.0",
  "description": "Add a preview of the short PRs diff to the main PR page on Github.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://github.com/*/*/pull/*"
      ]
    }
  ]
}